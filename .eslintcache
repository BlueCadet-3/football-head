 to ESPN API with provided league ID and most recent season\n      const data = await initSeason(formData.league, formData.year);\n      // update user \"seasons\" property with previous seasons as indicated by ESPN API\n      console.log(\"user:\", user);\n      // let updatedUser = await getPastSeasons(data.status.previousSeasons);\n      let updatedUser = await getPastSeasons(data);\n      // setUser to the user with \"seasons\" data\n      \n      console.log(\"updatedUser:\", updatedUser);\n      this.props.setUser(updatedUser);\n    } catch {\n      this.setState({ error: \"Sign Up Failed - Try Again\" });\n    }\n  };\n\n  render() {\n    const disable = this.state.password !== this.state.confirm;\n    return (\n      <div>\n        <div className=\"form-container\">\n          <form autoComplete=\"off\" onSubmit={this.handleSubmit}>\n            <label htmlFor=\"email\">Email</label>\n            <input\n              type=\"email\"\n              name=\"email\"\n              id=\"email\"\n              value={this.state.email}\n              onChange={this.handleChange}\n              placeholder=\"Required\"\n              required\n            />\n\n            <label htmlFor=\"league\">\n              League ID &nbsp;\n              <div className=\"tooltip\">\n                <FontAwesomeIcon icon={faQuestionCircle}>\n                  <span className=\"tooltiptext\">\n                    <img\n                      src=\"https://www.scriptol.com/images/apache.png\"\n                      alt=\"League ID Tooltip\"\n                    />\n                    <h3>How use my site</h3>\n                    The description with an image.\n                  </span>\n                </FontAwesomeIcon>\n              </div>\n            </label>\n\n            <input\n              type=\"number\"\n              name=\"league\"\n              id=\"league\"\n              value={this.state.league}\n              onChange={this.handleChange}\n              placeholder=\"Required\"\n              required\n            />\n\n            <label htmlFor=\"year\">\n              Year &nbsp;\n              <div className=\"tooltip\">\n                <FontAwesomeIcon icon={faQuestionCircle}></FontAwesomeIcon>\n              </div>\n            </label>\n\n            {/* <select\n              name=\"year\"\n              id=\"year\"\n              onChange={this.handleChange}\n              value={this.state.year}\n            >\n              <option selected value={2020}>\n                2020\n              </option>\n              <option value={2019}>2019</option>\n              <option value={2018}>2018</option>\n            </select> */}\n\n            <input\n              type=\"number\"\n              name=\"year\"\n              id=\"year\"\n              value={this.state.year}\n              onChange={this.handleChange}\n              placeholder=\"Required\"\n              required\n            />\n\n            <label htmlFor=\"team\">\n              Team ID &nbsp;\n              <FontAwesomeIcon icon={faQuestionCircle} />\n            </label>\n            <input\n              type=\"number\"\n              name=\"team\"\n              id=\"team\"\n              value={this.state.team}\n              onChange={this.handleChange}\n              placeholder=\"Required\"\n              required\n            />\n            <label htmlFor=\"password\">Password</label>\n            <input\n              type=\"password\"\n              name=\"password\"\n              id=\"password\"\n              value={this.state.password}\n              onChange={this.handleChange}\n              placeholder=\"Required\"\n              required\n            />\n            <label htmlFor=\"confirm\">Confirm</label>\n            <input\n              type=\"password\"\n              name=\"confirm\"\n              id=\"confirm\"\n              value={this.state.confirm}\n              onChange={this.handleChange}\n              placeholder=\"Required\"\n              required\n            />\n            <button className=\"AuthPage\" type=\"submit\" disabled={disable}>\n              SIGN UP\n            </button>\n          </form>\n        </div>\n        <p className=\"error-message\">&nbsp;{this.state.error}</p>\n      </div>\n    );\n  }\n}\n","/home/linux/github/football-head/src/components/UserLogOut/UserLogOut.jsx",[],["68","69"],"/home/linux/github/football-head/src/components/LoginForm/LoginForm.jsx",[],"/home/linux/github/football-head/src/utilities/send-request.js",[],"/home/linux/github/football-head/src/components/NavBar/NavBar.jsx",[],"/home/linux/github/football-head/src/components/YearBar/YearBar.jsx",[],"/home/linux/github/football-head/src/utilities/espn-api.js",[],"/home/linux/github/football-head/src/pages/Dashboard/Dashboard.jsx",["70"],"/home/linux/github/football-head/src/pages/Season/Season.jsx",[],{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","severity":1,"message":"76","line":2,"column":18,"nodeType":"77","messageId":"78","endLine":2,"endColumn":23},{"ruleId":"71","replacedBy":"79"},{"ruleId":"73","replacedBy":"80"},{"ruleId":"75","severity":1,"message":"81","line":2,"column":10,"nodeType":"77","messageId":"78","endLine":2,"endColumn":19},"no-native-reassign",["82"],"no-negated-in-lhs",["83"],"no-unused-vars","'login' is defined but never used.","Identifier","unusedVar",["82"],["83"],"'useEffect' is defined but never used.","no-global-assign","no-unsafe-negation"]